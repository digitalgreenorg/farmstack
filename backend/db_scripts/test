SELECT datahub_userorganizationmap.id, datahub_userorganizationmap.created_at, datahub_userorganizationmap.updated_at, datahub_userorganizationmap.user_id_id, datahub_userorganizationmap.organization_id_id, accounts_user.created_at, accounts_user.updated_at, accounts_user.id, accounts_user.email, accounts_user.first_name, accounts_user.last_name, accounts_user.phone_number, accounts_user.role_id, accounts_user.profile_picture, accounts_user.status, accounts_user.subscription, datahub_organization.created_at, datahub_organization.updated_at, datahub_organization.id, datahub_organization.name, datahub_organization.org_email, datahub_organization.address, datahub_organization.phone_number, datahub_organization.logo, datahub_organization.hero_image, datahub_organization.website, datahub_organization.status FROM datahub_userorganizationmap INNER JOIN accounts_user ON (datahub_userorganizationmap.user_id_id = accounts_user.id) INNER JOIN datahub_organization ON (datahub_userorganizationmap.organization_id_id = datahub_organization.id